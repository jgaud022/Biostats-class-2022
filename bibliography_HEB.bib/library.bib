doi = {10.1007/s12010-020-03343-5},
file = {:Users/jgaud/Desktop/Readings/to read/plant/Mikucka-Zieli{\AA}ska2020{\_}Article{\_}DistilleryStillageCharacterist.pdf:pdf},
