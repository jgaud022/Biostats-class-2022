journal = {International Journal of Social Psychiatry},
keywords = {Chornobyl,depression,disaster,distress,epidemiology,mothers with young children,post-traumatic stress disorder,trauma},
number = {6},
pages = {637--645},
pmid = {21813484},
title = {{Psychological well-being and risk perceptions of mothers in Kyiv, Ukraine, 19 years after the Chornobyl disaster}},
volume = {57},
year = {2011}
}
