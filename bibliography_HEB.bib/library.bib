issn = {0749503X},
journal = {Yeast},
keywords = {Fermentation,Heterologous gene expression,Kluyveromyces lactis,Lactose-permease,Ribosomal DNA,Whey,$\beta$-galactosidase},
number = {9},
pages = {827--837},
pmid = {9818720},
title = {{Highly efficient assimilation of lactose by a metabolically engineered strain of Saccharomyces cerevisiae}},
