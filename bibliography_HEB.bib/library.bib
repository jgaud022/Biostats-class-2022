journal = {Environmental Health Perspectives},
keywords = {Cyanobacteria,Cyanotoxins,Cylindrospermopsins,Microcystins,Toxins},
number = {7},
pages = {663--668},
pmid = {11485863},
title = {{Human fatalities form cyanobacteria: Chemical and biological evidence for cyanotoxins}},
volume = {109},
year = {2001}
}
