issn = {03529568},
journal = {Chemical and Biochemical Engineering Quarterly},
keywords = {Biomass,Cultivation conditions,Ethanol,Fermentation,Kluyveromyces marxianus,Whey},
number = {1},
pages = {13--16},
title = {{Selection of yeast strain Kluyveromyces marxianus for alcohol and biomass production on whey}},
volume = {16},
year = {2002}
}
@article{Carmichael2001,
abstract = {An outbreak of acute liver failure occurred at a dialysis center in Caruaru, Brazil(8°17′S, 35°58″W), 134 km from Recife, the state capital of Pernambuco. At the clinic, 116(89{\%}) of 131 patients experienced visual disturbances, nausea, and vomiting after routine hemodialysis treatment on 13-20 February 1996. Subsequently, 100 patients developed acute liver failure, and of these 76 died. As of December 1996, 52 of the deaths could be attributed to a common syndrome now called Caruaru syndrome. Examination of phytoplankton from the dialysis clinic's water source, analyses of the clinic's water treatments system, plus serum and liver tissue of clinic patients led to the identification of two groups of cyanobacterial toxins, the hepatotoxic cyclic peptide microcystins and the hepatotoxic alkaloid cylindrospermopsin. Comparison of victims' symptoms and pathology using animal studies of these two cyanotoxins leads us to conclude that the major contributing factor to death of the dialyses patients was intravenous exposure to microcystins, specifically microcystin-YR, -LR, and -AR. From liver concentrations and exposure volumes, it was estimated that 19.5 $\mu$g/L microcystin was in the water used for dialysis treatments. This is 19.5 times the level set as a guideline for safe drinking water supplies by the World Health Organization.},
author = {Carmichael, Wayne W. and Azevedo, Sandra M.F.O. and An, Ji Si and Molica, Renato J.R. and Jochimsen, Elise M. and Lau, Sharon and Rinehart, Kenneth L. and Shaw, Glen R. and Eaglesham, Geoff K.},
