issn = {1474547X},
journal = {The Lancet},
number = {9992},
pages = {469--478},
pmid = {26251392},
publisher = {Elsevier Ltd},
title = {{Long-term effects of radiation exposure on health}},
url = {http://dx.doi.org/10.1016/S0140-6736(15)61167-9},
volume = {386},
year = {2015}
}
