issn = {00992240},
journal = {Applied and Environmental Microbiology},
number = {1},
pages = {457--464},
pmid = {16391078},
title = {{Mercury methylation from unexpected sources: Molybdate-inhibited freshwater sediments and an iron-reducing bacterium}},
volume = {72},
year = {2006}
}
