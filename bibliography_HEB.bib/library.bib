file = {:Users/jgaud/Desktop/Readings/all papers/Diniz et al. Transcriptome analysis of the thermotolerant yeast kmarx under ethanol stress.pdf:pdf},
issn = {14320614},
journal = {Applied Microbiology and Biotechnology},
keywords = {Ethanol stress,Kluyveromyces marxianus,Membrane,Transcriptome},
number = {18},
