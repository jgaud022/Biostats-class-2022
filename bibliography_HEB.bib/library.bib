keywords = {Greek yogurt acid whey,cottage cheese acid whey,dairy coproducts,milk ultrafiltration permeate},
number = {5},
pages = {3978--3984},
pmid = {30879808},
publisher = {American Dairy Science Association},
title = {{Short communication: Composition of coproduct streams from dairy processing: Acid whey and milk permeate}},
