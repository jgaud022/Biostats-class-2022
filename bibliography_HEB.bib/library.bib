@article{Rubio-Texeira2006,
abstract = {Most microorganisms adapted to life in milk owe their ability to thrive in this habitat to the evolution of mechanisms for the use of the most abundant sugar present on it, lactose, as a carbon source. Because of their lactose-assimilating ability, Kluyveromyces yeasts have long been used in industrial processes involved in the elimination of this sugar. The identification of the genes conferring Kluyveromyces with a system for permeabilization and intracellular hydrolysis of lactose (LAC genes), along with the current possibilities for their transfer into alternative organisms through genetic engineering, has significantly broadened the industrial profitability of lactic yeasts. This review provides an updated overview of the general properties of Kluyveromyces LAC genes, and the multiple techniques involving their biotechnological utilization. Emphasis is also made on the potential that some of the latest technologies, such as the generation of transgenics, will have for a further benefit in the use of these and related genes. {\textcopyright} 2005 Elsevier Inc. All rights reserved.},
author = {Rubio-Texeira, Marta},
doi = {10.1016/j.biotechadv.2005.10.001},
file = {:Users/jgaud/Desktop/Readings/all papers/Rubio texeira. Endless versatility in biotechnological applications of K LAC genes2006.pdf:pdf},
issn = {07349750},
journal = {Biotechnology Advances},
keywords = {Cheese whey,Kluyveromyces,LAC12,LAC4,Lactose,$\beta$-galactosidases},
number = {2},
pages = {212--225},
pmid = {16289464},
title = {{Endless versatility in the biotechnological applications of Kluyveromyces LAC genes}},
volume = {24},
year = {2006}
