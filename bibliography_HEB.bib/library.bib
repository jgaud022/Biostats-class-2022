pmid = {28776098},
publisher = {Applied Microbiology and Biotechnology},
title = {{Transcriptome analysis of the thermotolerant yeast Kluyveromyces marxianus CCT 7735 under ethanol stress}},
volume = {101},
year = {2017}
}
