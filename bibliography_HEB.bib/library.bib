@article{Christensen2011,
abstract = {Ethanol production by K. marxianus in whey from organic cheese production was examined in batch and continuous mode. The results showed that no pasteurization or freezing of the whey was necessary and that K. marxianus was able to compete with the lactic acid bacteria added during cheese production. The results also showed that, even though some lactic acid fermentation had taken place prior to ethanol fermentation, K. marxianus was able to take over and produce ethanol from the remaining lactose, since a significant amount of lactic acid was not produced (1-2 g/l). Batch fermentations showed high ethanol yield ({\~{}}0.50 g ethanol/g lactose) at both 30°C and 40°C using low pH (4.5) or no pH control. Continuous fermentation of nonsterilized whey was performed using Ca-alginate-immobilized K. marxianus. High ethanol productivity (2.5-4.5 g/l/h) was achieved at dilution rate of 0.2/h, and it was concluded that K. marxianus is very suitable for industrial ethanol production from whey. {\textcopyright} 2010 Society for Industrial Microbiology.},
author = {Christensen, Anne Deen and K{\'{a}}d{\'{a}}r, Zs{\'{o}}fia and Oleskowicz-Popiel, Piotr and Thomsen, Mette Hedegaard},
doi = {10.1007/s10295-010-0771-0},
file = {:Users/jgaud/Desktop/Readings/all papers/Christensen et al. Production of bioethanol from organic whey using K.marx.pdf:pdf},
issn = {13675435},
journal = {Journal of Industrial Microbiology and Biotechnology},
keywords = {Ca-alginate,Cheese whey,Continuous fermentation,Immobilization,Kluyveromyces marxianus},
number = {2},
pages = {283--289},
pmid = {20632200},
title = {{Production of bioethanol from organic whey using Kluyveromyces marxianus}},
volume = {38},
year = {2011}
}
@article{Johannsen1980,
abstract = {Hybridization studies based on the prototrophic selection technique, involving the use of auxotrophic mutants of strains of all accepted species of the genus Kluyveromyces, are reported. Two main groups of mutually interfertile taxa were established within the genus. The first group comprises Kluyveromyces bulgaricus, K. cicerisporus, K. dobzhanskii, K. drosophilarum, K. fragilis, K. lactis; K. marxianus, K. phaseolosporus, K. vanudenii and K. wikenii. The second group consists of K. dobzhanskii, K. drosophilarum, K. lactis, K. vanudenii and K. wickerhamii. Hybrids were also detected in crosses involving K. drosophilarum and K. waltii as well as K. marxianus and K. thermotolerans. In terms of the concept of the biological species and in compliance with the requirements of the International Code of Botanical Nomenclature, taxa which hybridize with K. marxianus and form fertile recombinants at frequencies observed in intraspecific crosses, are accepted as varieties of K. marxianus. {\textcopyright} 1980 H. Veenman {\&} Zonen B.V.},
author = {Johannsen, Elzbieta},
doi = {10.1007/BF00444073},
file = {:Users/jgaud/Desktop/Readings/meh/Johannsen1980{\_}Article{\_}HybridizationStudiesWithinTheG.pdf:pdf},
